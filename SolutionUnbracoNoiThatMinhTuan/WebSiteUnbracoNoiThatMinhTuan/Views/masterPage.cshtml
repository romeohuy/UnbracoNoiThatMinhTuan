@inherits Umbraco.Web.Mvc.UmbracoViewPage
@{
    Layout = null;
    Html.EnableClientValidation();
    Html.EnableUnobtrusiveJavaScript();
}

<!DOCTYPE html>
<!--[if IE 8 ]><html class="ie" xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en-US" lang="en-US">
<!--<![endif]-->



<head>
    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <title>@Page.Title</title>

    <meta name="subject" content="@Page.Title">
    <meta name="keywords" content="@Page.MetaKeywords" />
    <meta name="description" content="@Page.MetaDescription">

    <meta name="og:title" content="@Page.Title" />
    <meta name="og:url" content="@Request.Url" />
    <meta name="og:image" content="@Page.MainImageUrl" />
    <meta name="og:description" content="@Page.MetaDescription" />


    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Theme Style -->
    <link rel="stylesheet" type="text/css" href="~/themeassets/style.css">

    <!-- Colors -->
    <link rel="stylesheet" type="text/css" href="~/themeassets/assets/css/colors/color1.css" id="colors">

    <!-- Favicon and Touch Icons  -->
    <link rel="shortcut icon" href="~/themeassets/assets/icon/favicon.ico">
    <link rel="apple-touch-icon" sizes="57x57" href="~/themeassets/assets/icon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="~/themeassets/assets/icon/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/themeassets/assets/icon/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/themeassets/assets/icon/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/themeassets/assets/icon/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/themeassets/assets/icon/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/themeassets/assets/icon/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/themeassets/assets/icon/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="~/themeassets/assets/icon/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="~/themeassets/assets/icon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/themeassets/assets/icon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/themeassets/assets/icon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/themeassets/assets/icon/favicon-16x16.png">
    <link rel="manifest" href="~/themeassets/assets/icon/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="~/themeassets/assets/icon/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link rel="apple-touch-icon-precomposed" href="~/themeassets/assets/icon/apple-touch-icon-158-precomposed.png">

    <!--[if lt IE 9]>
        <script src="javascript/html5shiv.js"></script>
        <script src="javascript/respond.min.js"></script>
    <![endif]-->

</head>

<body class="header-fixed page no-sidebar @(Page.BodyTagCss != null && Page.BodyTagCss != "" ? "header-style-3 topbar-style-3" : "header-style-2 topbar-style-2" ) @Page.BodyTagCss menu-has-search">

    <div id="wrapper" class="animsition">
        <div id="page" class="clearfix">
            <!-- Header Wrap -->
            @Html.Partial("~/Views/Partials/HeaderPartial.cshtml")
            <!-- #site-header-wrap -->
            @RenderSection("FeaturedTitleBlock", required: false)
            <!-- Main Content -->
            <div id="main-content" class="site-main clearfix">
                <div id="content-wrap">
                    <div id="site-content" class="site-content clearfix">
                        <div id="inner-content" class="inner-content-wrap">
                            <div class="page-content">
                                <!--Main content page-->
                                @RenderBody()
                                <!--End Main content page-->
                            </div><!-- /.page-content -->
                        </div><!-- /#inner-content -->
                    </div><!-- /#site-content -->
                    @RenderSection("RightSideBlogs", required: false)
                </div><!-- /#content-wrap -->
            </div><!-- /#main-content -->
            <!-- Footer -->

            @Html.Partial("~/Views/Partials/FooterPartial.cshtml")
        </div><!-- /#page -->
    </div><!-- /#wrapper -->

    <a id="scroll-top"></a>

    <button id="btnDangKyThiCong" type="button" class="btn btn-warning" data-toggle="modal" data-target="#dangkythicongModal" data-whatever="">Đăng ký tư vấn</button>
    <div class="modal fade" id="dangkythicongModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Đăng ký tư vấn thiết kế và thi công</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    @{ Html.RenderAction("RenderForm", "ContactSurface");}
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Javascript -->
    <script src="~/themeassets/assets/js/jquery.min.js"></script>
    <script src="~/themeassets/assets/js/plugins.js"></script>
    <script src="~/themeassets/assets/js/tether.min.js"></script>
    <script src="~/themeassets/assets/js/bootstrap.min.js"></script>
    <script src="~/themeassets/assets/js/animsition.js"></script>
    <script src="~/themeassets/assets/js/owl.carousel.min.js"></script>
    <script src="~/themeassets/assets/js/countto.js"></script>
    <script src="~/themeassets/assets/js/equalize.min.js"></script>
    <script src="~/themeassets/assets/js/jquery.isotope.min.js"></script>
    <script src="~/themeassets/assets/js/owl.carousel2.thumbs.js"></script>

    <script src="~/themeassets/assets/js/jquery.cookie.js"></script>
    <script src="~/themeassets/assets/js/gmap3.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAIEU6OT3xqCksCetQeNLIPps6-AYrhq-s&amp;region=GB"></script>
    <script src="~/themeassets/assets/js/shortcodes.js"></script>
    <script src="~/themeassets/assets/js/main.js"></script>

    <!-- Revolution Slider -->
    <script src="~/themeassets/includes/rev-slider/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/themeassets/assets/js/rev-slider.js"></script>
    <!-- Load Extensions only on Local File Systems ! The following part can be removed on Server for On Demand Loading -->
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.actions.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.carousel.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.kenburn.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.migration.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.navigation.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.parallax.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script src="~/themeassets/includes/rev-slider/js/extensions/revolution.extension.video.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/scripts/jquery.validate.min.js"></script>

    <script src="~/scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/scripts/contactForm.js"></script>
    @RenderSection("ExtendScript", required: false)
</body>


</html>



